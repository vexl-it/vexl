{
  "name": "account-deletion-website",
  "type": "module",
  "scripts": {
    "dev": "node ./server.js",
    "start": "cross-env NODE_ENV=production node ./server.js",
    "build": "yarn remix vite:build",
    "clean": "rimraf .turbo node_modules",
    "lint": "eslint '**/*.{js,ts,tsx,jsx,cjs,mjs}'",
    "format": "prettier --check \"**/*.{js,mjs,cjs,jsx,ts,tsx,md,json}\"",
    "format:fix": "prettier --write \"**/*.{js,mjs,cjs,jsx,ts,tsx,md,json}\"",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@remix-run/css-bundle": "^2.8.1",
    "@remix-run/express": "^2.8.1",
    "@remix-run/node": "^2.8.1",
    "@remix-run/react": "^2.8.1",
    "@remix-run/serve": "^2.8.1",
    "@vexl-next/cryptography": "0.0.0",
    "@vexl-next/domain": "0.0.0",
    "@vexl-next/localization": "0.0.0",
    "@vexl-next/resources-utils": "0.0.0",
    "@vexl-next/rest-api": "0.0.0",
    "brorand": "^1.1.0",
    "browserify-zlib": "^0.2.0",
    "buffer": "^6.0.3",
    "cross-env": "^7.0.3",
    "crypto-browserify": "^3.12.0",
    "effect": "^3.10.12",
    "events": "^3.3.0",
    "express": "^4.21.1",
    "fp-ts": "^2.13.1",
    "isbot": "4",
    "process": "^0.11.10",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "stream-browserify": "^3.0.0",
    "util": "^0.12.5",
    "zod": "^3.22.3"
  },
  "devDependencies": {
    "@remix-run/dev": "^2.8.1",
    "@types/brorand": "^1",
    "@types/events": "^3",
    "@types/express": "^4",
    "@types/react": "~18.2.45",
    "@types/react-dom": "^18",
    "@vexl-next/eslint-config": "0.0.0",
    "@vexl-next/prettier-config": "0.0.0",
    "@vexl-next/tsconfig": "0.0.0",
    "eslint": "^8.57.1",
    "prettier": "^3.3.2",
    "typescript": "^5.6.3",
    "vite": "^5.4.12",
    "vite-plugin-node-polyfills": "^0.21.0"
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "@vexl-next/eslint-config/index"
    ],
    "ignorePatterns": [
      "build"
    ]
  },
  "prettier": "@vexl-next/prettier-config"
}
