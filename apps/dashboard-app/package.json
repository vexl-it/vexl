{
  "name": "@vexl-next/dashboard-app",
  "main": "dist/server/index.cjs",
  "type": "module",
  "scripts": {
    "clean": "rimraf ./dist",
    "format": "prettier --check \"**/*.{js,mjs,cjs,jsx,ts,tsx,md,json}\"",
    "format:fix": "prettier --write \"**/*.{js,mjs,cjs,jsx,ts,tsx,md,json}\"",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "typecheck": "tsc --noEmit",
    "build": "yarn typecheck && yarn build:server && yarn build:client",
    "build:client": "vite build",
    "build:server": "node esbuild.config.js",
    "dev": "yarn dev:server & yarn dev:client",
    "dev:server": "npx tsx server/index.ts",
    "dev:client": "vite dev",
    "dev:client:preview": "vite preview --port 5173",
    "dev:watch": "npx tsx --watch server/index.ts & vite dev",
    "start": "node dist/server/index.cjs"
  },
  "dependencies": {
    "@effect/experimental": "^0.57.11",
    "@effect/platform": "^0.93.6",
    "@effect/platform-node": "^0.103.0",
    "@effect/sql": "^0.48.6",
    "@effect/sql-pg": "^0.49.7",
    "@emotion/css": "^11.11.2",
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@sentry/esbuild-plugin": "^2.10.2",
    "@sentry/node": "^7.93.0",
    "@sentry/profiling-node": "^1.3.5",
    "@vexl-next/cryptography": "0.0.0",
    "@vexl-next/domain": "0.0.0",
    "@vexl-next/resources-utils": "0.0.0",
    "@vexl-next/rest-api": "0.0.0",
    "@vexl-next/server-utils": "0.0.0",
    "axios": "^1.8.2",
    "dayjs": "^1.11.11",
    "dotenv": "^16.4.5",
    "dynamic-import-polyfill": "^0.1.1",
    "effect": "^3.19.9",
    "fast-check": "^3.20.0",
    "jotai": "^2.15.0",
    "react": "19.1.2",
    "react-animated-numbers": "^0.18.0",
    "react-confetti": "^6.1.0",
    "react-dom": "19.1.2",
    "react-spring": "^9.7.3",
    "react-use": "^17.5.0",
    "serve-handler": "^6.1.5",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@formkit/auto-animate": "^0.8.2",
    "@types/http-proxy": "^1",
    "@types/react": "~19.1.10",
    "@types/react-dom": "~19.1.7",
    "@types/serve-handler": "^6",
    "@types/source-map-support": "^0.5.10",
    "@types/ws": "^8",
    "@vexl-next/eslint-config": "0.0.0",
    "@vexl-next/prettier-config": "0.0.0",
    "@vexl-next/tsconfig": "0.0.0",
    "@vitejs/plugin-react": "^4.3.0",
    "esbuild": "^0.25.0",
    "eslint": "^8.57.1",
    "http-proxy": "^1.18.1",
    "jest": "^29.7.0",
    "nodemon": "^2.0.21",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.3.2",
    "rimraf": "^4.4.0",
    "ts-jest": "^29.2.0",
    "ts-node": "^10.9.1",
    "typescript": "~5.9.2",
    "vite": "^6.1.6"
  },
  "prettier": "@vexl-next/prettier-config"
}
