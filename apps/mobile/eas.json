{
  "cli": {
    "version": ">=3.7.0",
    "promptToConfigurePushNotifications": false
  },
  "build": {
    "staging": {
      "android": {
        "credentialsSource": "remote"
      },
      "ios": {
        "credentialsSource": "remote"
      },
      "env": {
        "ENV_PRESET": "stage"
      },
      "channel": "staging"
    },
    "stagingApk": {
      "android": {
        "credentialsSource": "remote",
        "buildType": "apk"
      },
      "env": {
        "ENV_PRESET": "stage"
      },
      "channel": "staging-apk"
    },
    "production": {
      "android": {
        "credentialsSource": "remote"
      },
      "ios": {
        "credentialsSource": "remote"
      },
      "env": {
        "ENV_PRESET": "prod"
      },
      "channel": "production"
    },
    "productionApk": {
      "android": {
        "image": "ubuntu-20.04-jdk-11-ndk-r19c",
        "credentialsSource": "remote",
        "buildType": "apk"
      },
      "env": {
        "ENV_PRESET": "prod"
      },
      "channel": "production-apk"
    }
  },
  "submit": {
    "staging": {
      "android": {
        "track": "internal",
        "changesNotSentForReview": true,
        "serviceAccountKeyPath": "credentials/google.json"
      },
      "ios": {
        "bundleIdentifier": "it.vexl.nextstaging",
        "ascAppId": "6448746479",
        "appleTeamId": "KQNTW88PVA"
      }
    },
    "production": {
      "android": {
        "track": "internal",
        "changesNotSentForReview": true
      },
      "ios": {
        "bundleIdentifier": "it.vexl.next",
        "ascAppId": "6448051657",
        "appleTeamId": "KQNTW88PVA"
      }
    }
  }
}
