{
  "name": "@vexl-next/eslint-config",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "clean": "rm -rf .turbo node_modules",
    "lint": "eslint .",
    "format": "prettier --check \"**/*.{js,mjs,cjs,jsx,ts,tsx,md,json}\"",
    "format:fix": "prettier --write \"**/*.{js,mjs,cjs,jsx,ts,tsx,md,json}\"",
    "typecheck": "tsc --noEmit"
  },
  "exports": {
    ".": "./index.mjs",
    "./react": "./react.mjs"
  },
  "dependencies": {
    "eslint-config-love": "^134.0.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "globals": "^16.1.0",
    "typescript-eslint": "^8.32.1"
  },
  "files": [
    "index.mjs",
    "react.mjs"
  ],
  "devDependencies": {
    "@vexl-next/prettier-config": "0.0.0",
    "@vexl-next/tsconfig": "0.0.0",
    "eslint": "^9.27.0",
    "prettier": "^3.3.2",
    "typescript": "~5.9.2"
  },
  "prettier": "@vexl-next/prettier-config"
}
